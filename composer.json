{
    "name": "voorhof/etiquette",
    "description": "A collection of re-usable code across different Laravel projects, including classes and traits for package development",
    "keywords": ["laravel", "etiquette", "traits"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/voorhof/etiquette/issues",
        "source": "https://github.com/voorhof/etiquette"
    },
    "authors": [
        {
            "name": "David Carton",
            "email": "david@notrac.be"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/support": "^12.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "laravel/framework": "^12.0",
        "orchestra/testbench-core": "^10.0",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Voorhof\\Etiquette\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voorhof\\Etiquette\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test:coverage": "pest --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voorhof\\Etiquette\\EtiquetteServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
